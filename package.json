{
  "name": "alphavantage-lightweight-chart",
  "private": true,
  "version": "1.0.0",
  "description": "A basic React/Typescript/Vite application for fetching stock quotes from AlphaVantage and displaying historical data with the TradingView Lightweight Chart library.",
  "author": "Incremental Capitalist",
  "license": "LGPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "aws-amplify": "^6.5.3",
    "lightweight-charts": "4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.2.0",
    "@aws-amplify/backend-cli": "^1.2.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "autoprefixer": "^10.4.14",
    "aws-cdk": "^2.155.0",
    "aws-cdk-lib": "^2.155.0",
    "constructs": "^10.3.0",
    "esbuild": "^0.23.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite": "^4.2.0",
    "vitest": "^0.30.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}